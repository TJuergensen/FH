package stack;

public class ArrayStack<E> implements Stack<E> {
	
	private E[] array;
	private int top;
	private static final int CAPACITY = 10;
	
	ArrayStack()
	{
		array = (E[]) new Object[CAPACITY];
		top = -1;
	}
	
	
	
	@Override
	public boolean isEmpty() {
		// TODO Auto-generated method stub
		return top == -1;
	}
	@Override
	public void push(E e) {
		array[++top] = e;
	}
	@Override
	public void pop() {
		array[top--] = null;
	}
	@Override
	public E top() {
		return array[top];
	}
	@Override
	public E poptop() {
		// TODO Auto-generated method stub
		return null;
	}

}
