package stack;

import stack.ListStack.ListStack;
import stack.ListStack.Stack;

public class PostFixInterpreter {
    
    private ListStack<Character> stack;
    
    public PostFixInterpreter() {
	stack = new ListStack<Character>();
    }
    
    public int interpret(String s)
    {
	int result = 0;
	fillStack(stack, s);
	
	return result;
    }
    
    private void fillStack(Stack<Character> stack, String s)
    {
	for(int i=0; i< s.length(); i++)
	{
	    stack.push(s.charAt(i));
	}	
    }

}
