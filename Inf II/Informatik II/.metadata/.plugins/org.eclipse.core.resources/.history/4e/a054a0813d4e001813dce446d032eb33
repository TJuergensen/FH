package stackTest;

import static org.junit.Assert.assertFalse;
import static org.junit.Assert.assertTrue;

import java.util.List;

import org.junit.jupiter.api.Test;

import stack.ArrayStack.ArrayStack;

class StackTest {
   
    ArrayStack<Integer> stack(List<Integer> data) {
	return null;
    }

    // isEmpty(push(x,s)) = false
    @Test
    public void isEmpty_push() {
	ArrayStack<Integer> stack = new ArrayStack<Integer>(1);
	stack.push(5);
	//assertTrue
	assertFalse(stack.isEmpty());
    }
    // isEmpty() == true
    @Test
    public void isEmpty_true()
    {
	ArrayStack<Integer> stack = new ArrayStack<Integer>(1);
	assertTrue(stack.isEmpty());
	stack.push(5);
	stack.pop();
	assertTrue(stack.isEmpty());
    }
    // top(push(x,s)) = x
    @Test
    public void popTop_top()
    {
	ArrayStack<Integer> stack = new ArrayStack<Integer>(2);
	
    }
    // popTop(s) = top(s), ignore

}
