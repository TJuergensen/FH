package stack.ListStack;

import java.util.ArrayList;

public class ListStack<E> implements Stack<E> {

	private Cell<E> top;
	private ArrayList<Cell> list;
	ListStack()
	{
		top = new Cell<E>(null);
		list  = new ArrayList<Cell>();
		list.add(top);
	}

	@Override
	public boolean isEmpty()
	{
		return top == null;
	}

	@Override
	public void push(E e)
	{
		Cell<E> newCell = new Cell<E>(e);
		list.add(newCell);
	}

	@Override
	public void pop()
	{

	}

	@Override
	public E top()
	{
		return top.content;
	}

	@Override
	public E poptop()
	{
		// TODO Auto-generated method stub
		return null;
	}

}
